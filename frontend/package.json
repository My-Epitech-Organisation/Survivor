{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:unused": "ts-prune",
    "lint:fix-all": "npm run lint:fix && npm run lint:fix-imports && npm run lint:fix-hooks",
    "lint:fix-imports": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule 'unused-imports/no-unused-imports: error'",
    "lint:fix-hooks": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule 'react-hooks/exhaustive-deps: error'",
    "dev": "npm run dev:frontend",
    "dev:frontend": "next dev --turbopack -H 0.0.0.0",
    "dev:backend": "cd ../backend && docker build -t survivor-backend . && docker run --rm -p 8000:8000 -v $(pwd):/app survivor-backend",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "serve": "npx serve@latest out",
    "preview": "npm run build && npm run serve",
    "start": "node app/"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-pdf/renderer": "^4.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/puppeteer": "^5.4.7",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "puppeteer": "^24.18.0",
    "puppeteer-core": "^24.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "tailwindcss": "^4",
    "ts-prune": "^0.10.3",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
