{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "next dev --turbopack -H 0.0.0.0",
    "dev:backend": "cd ../backend && docker build -t survivor-backend . && docker run --rm -p 8000:8000 -v $(pwd):/app survivor-backend",
    "build": "next build --turbopack",
    "serve": "npx serve@latest out",
    "preview": "npm run build && npm run serve",
    "start": "node app/"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
